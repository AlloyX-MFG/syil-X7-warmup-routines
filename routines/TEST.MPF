; TEST.MPF
; Test this routine before running other routines

EXTERN WARMUP_CYCLE(REAL, REAL)

G70 G90 G17  ; G70=inch mode (Siemens), G90=absolute positioning, G17=XY plane

; Move to safe position
G0 SUPA Z0        ; retract Z to machine zero (top of travel) first
G0 SUPA X0 Y0     ; then move X and Y to position 1

WARMUP_CYCLE(3600, 5)   ; 30% for 5 sec
WARMUP_CYCLE(7200, 5)   ; 60% for 5 sec

M5                      ;SPINDLE STOP

; Return to safe position
G0 SUPA Z0        ; retract Z to machine zero (top of travel)
G0 SUPA X0 Y0     ; move X and Y to position 1

M30                ;end of program